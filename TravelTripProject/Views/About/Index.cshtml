@using TravelTripProject.Models.Classes
@model List<About>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_TestPartial.cshtml";
}
<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>About</h3>
            @foreach (var x in Model)
            {
                <img src="@x.ImgUrl" alt="" />
                <p>@x.Aciklama</p>
            }


        </div>
    </div>
</div>

