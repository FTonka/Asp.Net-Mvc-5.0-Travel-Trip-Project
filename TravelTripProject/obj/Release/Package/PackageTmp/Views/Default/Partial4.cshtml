@using TravelTripProject.Models.Classes
@model List<Blog>
<div class="padding1">
    <h4>En İyi Gezilerimiz</h4>
    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">
                <img src="@item.BlogImg" alt="" />
            </div>
            <div class="bot-desc">
                <p>@item.Aciklama.Substring(0, 80)...</p>
            </div>
            <div class="clear"></div>
        }
        <div class="clear"></div>   
    </div>
    @Html.Action("Partial5","Default")
</div>
